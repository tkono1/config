[user]
	name = ${USER}
	email = <email>
	signingkey = <signing key ID, gpg --list-secret-keys --keyid-format LONG>
[color]
	ui = auto
[core]
	editor = vim
	excludesfile = ${HOME}/.config/git/ignore
	autocrlf = input
	quotepath = false
	ignorecase = false
[alias]
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
[gpg]
	program = gpg
[commit]
	gpgsign = true
[pull]
	rebase = false
